# SensAI API Environment Variables
# Copy this file to .env and fill in your actual values

# Required Environment Variables
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here

# Optional Environment Variables
ENV=development
S3_BUCKET_NAME=your_s3_bucket_name
S3_FOLDER_NAME=your_s3_folder_name

# Monitoring and Error Tracking
BUGSNAG_API_KEY=your_bugsnag_api_key
PHOENIX_ENDPOINT=your_phoenix_endpoint
PHOENIX_API_KEY=your_phoenix_api_key

# Slack Webhooks (Optional)
SLACK_USER_SIGNUP_WEBHOOK_URL=your_slack_user_signup_webhook_url
SLACK_COURSE_CREATED_WEBHOOK_URL=your_slack_course_created_webhook_url
SLACK_USAGE_STATS_WEBHOOK_URL=your_slack_usage_stats_webhook_url

# Database Configuration (SQLite is used by default)
# For production, consider using PostgreSQL
# DATABASE_URL=postgresql://user:password@localhost/dbname

# Redis Configuration (Optional, for caching)
# REDIS_URL=redis://localhost:6379

# Application Settings
# LOG_LEVEL=INFO
# DEBUG=false
# WORKERS=4 